@model MyApp.Web.Data.Entities.CompanyType

@{
    ViewData["Title"] = "Borrar Tipo de Empresa";
}

<h2>Borrar Tipo de Empresa</h2>

<h3>Está seguro de borrar este Tipo de Empresa?</h3>
<div>
    
    <hr />
    
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borrar" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-success">Regresar</a>
        
    </form>
</div>
